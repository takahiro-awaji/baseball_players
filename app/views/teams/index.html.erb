<h1>Teams#index</h1>
<p>Find me in app/views/teams/index.html.erb</p>
<% if team_signed_in? %>
<%= link_to "マイチームページ", team_path(current_team) %>
<%= link_to "ログアウト", destroy_team_session_path, method: :delete %>
<% else %>
<%= link_to "新規登録", new_team_registration_path %>
<%= link_to "ログイン", team_session_path %>
<% end %>
<ul>
  <% @teams.each do |team| %>
    <li><%= link_to team.team_name, team_path(team) %></li>
  <% end %>
</ul>